{
  "name": "sspa-root",
  "version": "1.0.0",
  "description": "A wrapper application for our console micro services that contains the SingleSPA",
  "main": "index.js",
  "scripts": {
    "build:prod": "rm -rf ./build/*; cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --color",
    "build:dev": "rm -rf ./build/*; cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js --color",
    "serve": "webpack serve -c ./webpack/webpack.serve.js",
    "npm-install": "npm install"
  },
  "author": "Or Shalmayev",
  "homepage": "",
  "dependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.3",
    "babel-loader": "8.0.6",
    "core-js": "3.8.1",
    "empty-module": "0.0.2",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "image-webpack-loader": "7.0.1",
    "import-map-overrides": "2.4.2",
    "lodash": "4.17.21",
    "moment": "2.22.2",
    "regenerator-runtime": "0.13.3",
    "single-spa": "5.5.0",
    "symbol-observable": "4.0.0",
    "systemjs": "6.2.0",
    "webpack": "5.15.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "webpack-dev-server": "4.5.0",
    "cross-env": "7.0.0",
    "webpack-cli": "^4.9.1",
    "css-loader": "5.0.1",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "3.1.2",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "mini-css-extract-plugin": "1.3.6"
  },
  "engines": {
    "node": ">=12.13.0 <14"
  }
}
